https://leetcode.com/problems/longest-consecutive-sequence/

var longestConsecutive = function(nums) {
    let set = new Set(nums);
    let length = 0;
   
    for(let num of nums) {
        if(!set.has(num-1)) {
            let count = 1;
            let temp = num+1;
            while(set.has(temp)) {
                count++;
                temp++;
            }
            length = Math.max(count, length);
        }
    }
    return length;
};
