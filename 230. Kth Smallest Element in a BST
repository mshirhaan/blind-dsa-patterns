https://leetcode.com/problems/kth-smallest-element-in-a-bst/

var kthSmallest = function(root, k) {
    let result = [];
    function dfs(root) {
        if(!root) return;
        dfs(root.left);
        result.push(root.val);
        dfs(root.right);
    }
    dfs(root);
    return result[k-1];
};
