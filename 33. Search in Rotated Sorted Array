var search = function(nums, target) {
    let l = 0;
    let r = nums.length-1;
    
    while(l<=r) {
        let mid = Math.floor((l+r)/2);
        if(nums[mid] == target) {
            return mid;
        }
        if(nums[mid] >= nums[l] && nums[mid] > target) {
            if(nums[l] <= target) {
                r = mid - 1;
            } else {
                l = mid + 1;
            }
        } else if(nums[mid] >= nums[l] && nums[mid] < target) {
            l = mid+1;
        } else if(nums[mid] < nums[l] && nums[mid] < target) {
            if(target > nums[r]) {
                r = mid - 1;
            } else {
                l = mid + 1;
            }
        } else if(nums[mid] < nums[l] && nums[mid] > target) {
            r = mid - 1;
        }
    }
    return -1;
};
