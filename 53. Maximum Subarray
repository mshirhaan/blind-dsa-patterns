var maxSubArray = function(nums) {
    let sum = nums[0];
    let maxSum = nums[0];
    
    for(let i = 1; i<nums.length; i++) {
        let num = nums[i];
        if(num > sum + num) {
            sum = num;
        } else {
            sum+=num;
        }
        
        if(sum>maxSum) maxSum = sum;
    }
    
    return maxSum;
};
