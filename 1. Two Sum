var twoSum = function(nums, target) {
    let map = {};
    
    for(let i = 0; i < nums.length; i++) {
        if(map[target - nums[i]] != undefined) { // if first index is stored as 0, if condition can fail, that is why check undefined
            return [i, map[target - nums[i]]];
        }
        map[nums[i]] = i;
    }
};
