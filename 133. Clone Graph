var cloneGraph = function(node) {
    if(node==null) return;
    let map = {};
    
    function dfs(node) {
        if(map[node.val]) return map[node.val];
        
        let cloneNode = new Node(node.val);
        map[node.val] = cloneNode;
        for(let nei of node.neighbors) {
            cloneNode.neighbors.push(dfs(nei));
        }
        return cloneNode;
    }
    
    return dfs(node);
};
