https://leetcode.com/problems/valid-parentheses/

var isValid = function(s) {
    let stack = [];
    for (let i = 0; i < s.length; i++) {
        if (s[i] == "(" || s[i] == "{" || s[i] == "[") {
            stack.push(s[i]);
        } else {
            let b = stack.pop();
            if ((!b || b == '{' && s[i] != '}') || (b == '(' && s[i] != ')') || (b == '[' && s[i] != ']')) {
                return false;
            }
        }
    }
    return stack.length == 0;
};
